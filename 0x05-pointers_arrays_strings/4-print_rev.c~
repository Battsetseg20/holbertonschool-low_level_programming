#include "holberton.h"

/**
 * _strlen - finds the length of a string
 *print_rev - prints a string in reverse order
 *@s: string variable
 *Return: void
 */


int _strlen(char *s)
{
	int i, c = 0;

	for (i = 0; *(s + i) != '\0'; i++)
	{
		c++;
	}
	return (i);
}

void print_rev(char *s)
{
	int i, c;

	c = _strlen(s)

		for (i = (c - 1); i >= 0; i--)
		{
			_putchar(*(s + i));
		}
	_putchar('\n');
}
