#include <stdio.h>
#include <stdlib.h>
#include <time.h>

/**
 *main - generates random password for 101-crackme program
 *
 *101-crackme password is compared with the sum of the values equal to 0xad4
 *use objdump to locate the value
 *0xad4 equal to 2772 in decimal
 *sum of the ASCII values from 32 to 122 are choses to generate password
 *
 *Return: success
 */

int main(void)
{
	int r, sum;
	int l = 2772-122;

	srand(time(0));

	for (sum = 0; sum <= l;sum += r)
	{
		r = rand() % 122;
		if (r > 32)
		{
			putchar(r);
		}
	}
	putchar(2772-sum);
	return (0);
}
