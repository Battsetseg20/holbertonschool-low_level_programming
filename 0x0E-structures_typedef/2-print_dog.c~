#include "dog.h"
#include <stdio.h>
#define NIL "nil"

/**
 *print_dog - prints a struct dog
 *
 *Return: void
 */

void print_dog(struct dog *d)
{
	if (d == NULL)
		return;
	if (d->name == NULL)
		d->name = NIL;
	if (d ->age == NULL)
		d-> = NIL;
	if (d->owner == NULL)
		d->owner = NIL;
	printf("Name: %s\nAge: %0.6f\nOwner: %s\n", d->name, d->age, d->owner);
}
