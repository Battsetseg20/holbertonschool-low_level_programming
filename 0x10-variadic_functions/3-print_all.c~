#include "variadic_functions.h"
#include <stdio.h>
#include <stdarg.h>

/**
 *print_all - prints any argument of the function
 *@format: list of types of arguments passed to the function
 *
 *Return: void
 */

void print_all(const char* format, ...)
{
	int ival, cval;
	double dval;
	char *sval;
	va_list args;

	va_start(args, format);

	while(*format != '\0')
	{
		switch(*format)
		{
		case 'i':
			ival = va_arg(args, int);
			printf("%i", i);
			break;
		case 'c':
			cval = va_arg(args, int);
			printf("%c", c);
			break;
		case 'f':
			d = va_arg(args, double);
			printf("%f", d);
			break;
		case 's':
			sval = va_arg(args, char *);
			printf("%s", s);
			break;
		}
		if (*format == 'c' || *format == 'i' ||
		     *format == 'i' || *format == 'f' || *format == 's'
		    && *(format + 1) != '\0')
			printf(", ");
		++format;
	}
	va_end(args);
}
