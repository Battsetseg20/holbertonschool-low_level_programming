#include "variadic_functions.h"
#include <stdio.h>
#include <stdarg.h>



void print_strings(const char *separator, const unsigned int n, ...)
{
	va_list str_list;
	unsigned int i;
	char *str;

	va_start(str_list, n);
	for (i = 0; i < n; i++)
	{
		str = va_arg(str_list, char *);
		if (str == NULL)
			printf("nil");
		printf("%s", str);
		if (separator && i < (n - 1))
			printf("%s", separator);
	}
	va_end(str_list);
	printf("\n");
}
